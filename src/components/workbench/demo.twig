<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, user-scalable=yes">
    <title>Workbench demo</title>
    <link rel="stylesheet" href="../css/index.css">
</head>
<body>
<div id="app">
    <demo title="Workbench" v-bind:directions="{{ directions|json_encode|escape }}">
        <div slot="content" class="demo-content">
            <div class="demo-workbench">
                <demo-workbench inline-template>
                    <workbench :title="'Lorem ipsum'" :components="{{ components|json_encode|escape }}"
                               :devices="{{ devices|json_encode|escape }}"></workbench>
                </demo-workbench>
            </div>
        </div>
    </demo>
</div>
<script src="../js/index.js"></script>
</body>
</html>
