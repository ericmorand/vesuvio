    <meta name="viewport" content="width=device-width, user-scalable=yes">
    <title>Toolbar demo</title>
    <link rel="stylesheet" href="index.css">
</head>
<body>
<div id="app">
    <demo title="Toolbar" v-bind:directions="{{ directions|json_encode|escape }}">
